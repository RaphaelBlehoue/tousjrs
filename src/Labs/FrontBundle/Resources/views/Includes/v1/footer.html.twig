                                            <a href="{{ path('page_view', {'slug': article.slug }) }}" rel="{{ article.item.name }}" title="{{ article.name }}">{{ article.name|truncate('120', false, '...') }}</a>
                                        </h3>
                                        <div class="td-module-meta-info">
                                            <span class="td-post-date"><time class="entry-date updated td-module-date" datetime="{{ article.created.date }}" >{{ article.created|localizeddate('none', 'none', null, null, 'dd MMMM Y') }}</time></span>
                                        </div>
                                    </div>
                                </div>
                            </div> <!-- ./td-block-span12 -->
                        {% endfor %}
