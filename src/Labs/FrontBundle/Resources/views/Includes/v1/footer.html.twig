<!--- footer --->
<div class="td-footer-wrapper">
    <div class="td-container">
        <div class="td-pb-row">
            <div class="td-pb-span12">
            </div>
        </div>
        <div class="td-pb-row">
            <div class="td-pb-span4">
                <div class="td_block_wrap td_block_7 td_uid_43_58eb574de7852_rand td-pb-border-top td_block_template_1"  data-td-block-uid="td_uid_43_58eb574de7852" >
                    <div class="td-block-title-wrap">
                        <h4 class="block-title"><span class="td-pulldown-size">{{ 'Derniers Dossiers'|upper }}</span></h4>
                    </div>
                    <div id=td_uid_43_58eb574de7852 class="td_block_inner">
                        {% for dossier in last_dossiers %}
                            <div class="td-block-span12">
                                <div class="td_module_6 td_module_wrap td-animation-stack">
                                    <div class="td-module-thumb">
                                        <a href="{{ url('page_dossier_view', {'rubrique': dossier.item.name , 'slug': dossier.slug}) }}" rel="{{ dossier.item.name }}" title="{{ dossier.name }}">
                                            {% for media in dossier.medias %}
                                                <img width="100" height="70" class="entry-thumb" src="{{ media.assertPath|imagine_filter('thumb_top_small') }}" srcset="{{ media.assertPath|imagine_filter('thumb_top_small') }} 100w, {{ media.assertPath|imagine_filter('thumb_top_small') }} 218w" sizes="(max-width: 100px) 100vw, 100px" alt="{{ dossier.name }}" title="{{ dossier.name }}"/>
                                            {% endfor %}
                                        </a>
                                    </div>
                                    <div class="item-details">
                                        <h3 class="entry-title td-module-title">
                                            <a href="{{ url('page_dossier_view', {'rubrique': dossier.item.name , 'slug': dossier.slug}) }}" rel="bookmark" title="{{ dossier.name }}">{{ dossier.name|truncate('120', false, '...') }}</a>
                                        </h3>
                                        <div class="td-module-meta-info">
                                            <span class="td-post-date"><time class="entry-date updated td-module-date" datetime="{{ dossier.created.date }}" >{{ dossier.created|localizeddate('none', 'none', null, null, 'dd MMMM Y') }}</time></span>
                                        </div>
                                    </div>
                                </div>
                            </div> <!-- ./td-block-span12 -->
                        {% endfor %}
                    </div>
                </div> <!-- ./block -->
            </div>

            <div class="td-pb-span4">
                <div class="td_block_wrap td_block_7 td_uid_44_58eb574dea200_rand td-pb-border-top td_block_template_1"  data-td-block-uid="td_uid_44_58eb574dea200" >
